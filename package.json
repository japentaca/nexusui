{
  "name": "nexusui",
  "version": "2.2.0",
  "description": "NexusUI: Web Audio Interfaces",
  "type": "module",
  "main": "dist/NexusUI.js",
  "module": "dist/NexusUI.esm.js",
  "exports": {
    ".": {
      "import": "./dist/NexusUI.esm.js",
      "require": "./dist/NexusUI.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "start": "vite",
    "docgen": "documentation build lib/** -f json -o api/api.json",
    "docserve": "vite preview --port 3000 --open",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint lib/**/*.js",
    "lint:fix": "eslint lib/**/*.js --fix",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nexus-js/ui"
  },
  "keywords": [
    "web-audio",
    "ui",
    "interface",
    "music",
    "audio",
    "javascript"
  ],
  "author": "Benjamin Taylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nexus-js/ui/issues"
  },
  "homepage": "https://nexus-js.github.io/ui",
  "dependencies": {
    "waaclock": "^0.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "color-ops": "^2.0.0",
    "documentation": "^14.0.0",
    "eslint": "^9.0.0",
    "fuse": "^0.12.1",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "standard-version": "^9.5.0",
    "vite": "^7.1.6"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ]
}
